{
  "name": "skms",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "XXbuildXX": "remix build",
    "build": "run-s \"build:*\"",
    "build:remix": "remix build",
    "build:css": "npm run generate:css",
    "generate:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "dev:remix": "remix dev",
    "dev:css": "npm run generate:css -- --watch",
    "dev:node": "cross-env NODE_ENV=development nodemon  --require dotenv/config --require  ./server.js --watch ./server.js",
    "XXdev": "npm run build && run-p dev:*",
    "dev": "run-p \"dev:*\"",
    "start": "node ./server.js"
  },
  "dependencies": {
    "@mcansh/remix-fastify": "^2.7.2",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "fastify": "^4.15.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@unocss/cli": "^0.51.8",
    "@unocss/preset-attributify": "^0.51.8",
    "@unocss/reset": "^0.51.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4",
    "unocss": "^0.51.8"
  },
  "engines": {
    "node": ">=14"
  }
}
